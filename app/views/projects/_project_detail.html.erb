<div class="project-detail"> 
  <div id="<%= dom_id project %>">
    <p>
      <strong>Name:</strong>
      <%= project.name %>
    </p>
    <p>
      <strong>Start Date:</strong>
      <%= project.start_date %>
    </p>
    <p>
      <strong>End Date:</strong>
      <%= project.end_date %>
    </p>
    <p>
      <strong>Description:</strong>
      <%= project.description %>
    </p>
  </div>
</div>